{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/prop-types": [0, {}],
    "react/no-unescaped-entities": [0, {}],
    "no-unused-vars": [1, {}]
  },
  "plugins": ["react"]
}
